{
  "openapi": "3.1.0",
  "info": {
    "title": "Vercel SQL Relay",
    "version": "1.0.0",
    "description": "Submit raw SQL to Supabase via a Vercel relay."
  },
  "servers": [
    {
      "url": "https://eidos-sql-relay.vercel.app",
      "description": "Relay server hosting the SQL endpoint"
    }
  ],
  "paths": {
    "/api/sql": {
      "post": {
        "operationId": "executeSQL",
        "summary": "Execute SQL via relay",
        "requestBody": {
          "required": true,
          "content": {
            "text/plain": {
              "schema": {
                "type": "string",
                "description": "Raw SQL query string"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Execution result",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object"
                      }
                    },
                    "error": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          }
        },
        "x-codegen-request-body-name": "rawSQL"
      }
    }
  }
}
